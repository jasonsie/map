{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "utilis/context.jsx",
    "utilis/useFetch.tsx",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "components/markerCluster.tsx",
    "components/trailMap.jsx",
    "components/navBar.jsx",
    "components/list.jsx",
    "components/markers.jsx"
  ],
  "exclude": ["node_modules"]
}
